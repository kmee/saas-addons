    <odoo>
        <record model="ir.ui.view" id="saas_migrate_operator_build">
            <field name="name">SaaS Migrate Operator Build</field>
            <field name="model">saas.migrate.operator.build</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="operator_id"/>
                    </group>
                    <footer>
                        <button string="Cancel" special="cancel" class="oe_highlight"/>
                        <button name="start_migration" string="Start Migration" type="object" class="oe_highlight" />
                    </footer>
                </form>
            </field>
        </record>
    </odoo>
