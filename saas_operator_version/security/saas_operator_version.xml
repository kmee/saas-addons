<odoo>

    <record model="ir.model.access" id="saas_operator_version_user_access">
        <field name="name">saas.operator.version user access</field>
        <field name="model_id" ref="model_saas_operator_version"/>
        <field name="group_id" ref="saas.group_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_create" eval="0"/>
        <field name="perm_write" eval="0"/>
        <field name="perm_unlink" eval="0"/>
    </record>

    <record model="ir.model.access" id="saas_operator_version_manager_access">
        <field name="name">saas.operator.version manager access</field>
        <field name="model_id" ref="model_saas_operator_version"/>
        <field name="group_id" ref="saas.group_manager"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

</odoo>
